.row(style='opacity: 1;')
  .col-lg-12
    #user-profile.row
      {view_info}
      .col-lg-3.col-md-4.col-sm-4
        .main-box.clearfix
          header.main-box-header.clearfix
            h2 {name} Ipad-2 {/}
          .main-box-body.clearfix
            .profile-status
              |  {stock_availability} Availabe In Stock {/}
            img.img-responsive.center-block(alt='', src='img/samples/ipad.png')
            .profile-label
              span.label.label-info Phones
            .profile-stars
              i.fa.fa-star
              i.fa.fa-star
              i.fa.fa-star
              i.fa.fa-star
              i.fa.fa-star-o
            .profile-since Added in List: {created_at} Jan 2012 {/}
            .profile-details
              ul.fa-ul
                li
                  i.fa-li.fa.fa-truck
                  | Orders: 
                  span {total_orders} 456 {/}
                li
                  i.fa-li.fa.fa-shopping-cart
                  | Sold: 
                  span {total_sale} 350 {/}
            .profile-message-btn.center-block.text-center
              a.btn.btn-success(href='#')
                i.fa.fa-edit
                | Edit
      {/}
      .col-lg-9.col-md-8.col-sm-8
        .main-box.clearfix
          .tabs-wrapper.profile-tabs
            ul.nav.nav-tabs
              li.active
                a(data-toggle='tab', href='#tab-Basic') Basic
              li
                a(data-toggle='tab', href='#tab-Attribute') Attribute
              li
                a(data-toggle='tab', href='#tab-QtyPrice') Qty&Price
              li
                a(data-toggle='tab', href='#tab-Category') Category
              li
                a(data-toggle='tab', href='#tab-Extra') Extra
              li
                a(data-toggle='tab', href='#tab-ProductionPhase') Production Phase
              li
                a(data-toggle='tab', href='#tab-Accounts') Accounts
            .tab-content
              #tab-Basic.tab-pane.fade.in.active
                | {basic_info}
                .row
                  .col-lg-12
                    .main-box.clearfix
                      header.main-box-header.clearfix
                        h2 Detail
                      .main-box-body.clearfix
                        .row
                          .col-sm-3
                            label  
                            strong Name:
                          .col-lg-3  {name} Wedding Cards {/}
                          .col-sm-3
                            label  
                            strong Code:
                          .col-lg-3  {sku} W1001 {/}
                        .xepan-push
                        .row
                          .col-lg-3
                            label
                            strong Status
                          .col-lg-3 Publish
                          .col-lg-3
                            label  
                            strong Date
                          .col-lg-3  {expiry_date} 25-2-2016 {/}
                        .xepan-push
                        .row
                          .col-lg-3
                            label  
                            strong Description:
                          .col-lg-9 {description} This Item to be sold. {/}
                        .xepan-push
                        .row
                          .col-lg-3
                            .checkbox-nice
                              | {is_template}
                              | {/}
                          .col-lg-3
                            label  
                            strong Designer:
                          .col-lg-3 Girnar Printers Pvt. Ltd
                          .col-lg-3 
                            .checkbox-nice
                              {$is_designable}
                    .row
                      .col-lg-12
                        .main-box
                          header.main-box-header.clearfix
                            h2 Item Type
                          .main-box-body.clearfix
                            .row
                              .col-lg-3
                                .checkbox-nice
                                  | {is_saleable}
                                  | {/}
                              .col-lg-3
                                .checkbox-nice
                                  | {is_purchasable}
                                  | {/}
                              .col-lg-3
                                .checkbox-nice
                                  | {is_productionable}
                                  | {/}
                              .col-lg-3
                                .checkbox-nice
                                  | {is_allowuploadable}
                                  | {/}
                    .row
                      .col-lg-12
                        .main-box
                          header.main-box-header.clearfix
                            h2 Item Stock Info
                          .main-box-body.clearfix
                            .row
                              .col-lg-4
                                .checkbox-nice
                                  | {maintain_inventory}
                                  | {/}
                              .col-lg-4
                                .checkbox-nice
                                  | {allow_negative_stock}
                                  | {/}
                .row
                  .col-lg-12
                    .main-box.clearfix
                      header.main-box-header.clearfix
                        h2 {$website_display}
                      .main-box-body.clearfix
                        .row
                          .col-lg-12
                            .main-box
                              header.main-box-header.clearfix
                                h2 Item Display Option
                              .main-box-body.clearfix
                                .row
                                  .col-lg-4
                                    .checkbox-nice
                                      | {show_detail}
                                      | {/}
                                  .col-lg-4
                                    .checkbox-nice
                                      | {show_price}
                                      | {/}
                                  .col-lg-4
                                    .checkbox-nice
                                      | {is_visible_sold}
                                      | {/}
                        .row
                          .col-lg-12
                            .main-box
                              header.main-box-header.clearfix
                                h2 Marked Option
                              .main-box-body.clearfix
                                .xepan-push
                                .row
                                  .col-lg-3
                                    .checkbox-nice
                                      | {is_new}
                                      | {/}
                                  .col-lg-3
                                    .checkbox-nice
                                      | {is_feature}
                                      | {/}
                                  .col-lg-3
                                    .checkbox-nice
                                      | {is_mostviewed}
                                      | {/}
                                  .col-lg-3
                                    {display_sequence} 5 {/}
                                    small.pull-left(style="width:80%") Display Sequence
                        .row
                          .col-lg-12
                            .main-box
                              header.main-box-header.clearfix
                                h2 Enquiry Option
                              .main-box-body.clearfix
                                .row
                                  .col-lg-4
                                    .checkbox-nice
                                      | {is_enquiry_allow}
                                      | {/}
                                  .col-lg-4
                                    .checkbox-nice
                                      | {enquiry_send_to_admin}
                                      | {/}
                                  .col-lg-4
                                    .checkbox-nice
                                      | {item_enquiry_auto_reply}
                                      | {/}
                        .row
                          .col-lg-12
                            .main-box
                              header.main-box-header.clearfix
                                h2 Item Comment Option
                              .main-box-body.clearfix
                                .row
                                  .col-lg-4
                                    .checkbox-nice
                                      | {is_comment_allow}
                                      | {/}
                                  .col-lg-2
                                    label  
                                    strong Comment:
                                  .col-lg-6  {comment_api} This Product is Genuine. {/}
                        .row
                          .col-lg-12
                            .main-box
                              header.main-box-header.clearfix
                                h2 Item Other Option
                              .main-box-body.clearfix
                                .row
                                  .col-lg-12
                                    .checkbox-nice
                                      | {add_custom_button}
                                      | {/}
                                .xepan-push
                                .row
                                  .col-lg-4
                                    label  
                                    strong Custom Button Label:
                                  .col-lg-2.input-full-width 
                                    | {custom_button_label} Ipad {/}
                                  .col-lg-3
                                    label  
                                    strong Custom Button Url:
                                  .col-lg-3.input-full-width
                                    | {custom_button_url} www.ios.com {/}
                    .row
                      .col-lg-12
                        .main-box
                          header.main-box-body.clearfix
                            .xepan-push
                            .row
                              .col-lg-3
                                label  
                                strong Terms & Condition:
                              .col-lg-9  {terms_and_conditions} This Item have not replacement warranty. {/}
                | {/}
              #tab-Attribute.tab-pane
                .col-lg-12.col-md-12.col-sm-12
                  .main-box.clearfix
                    .tabs-wrapper.profile-tabs
                      ul.nav.nav-tabs
                        li.active
                          a(data-toggle='tab', href='#tab-Specification') Specification
                        li
                          a(data-toggle='tab', href='#tab-CustomField') CustomField
                        li
                          a(data-toggle='tab', href='#tab-Filter') Filter
                      .tab-content
                        #tab-Specification.tab-pane.fade.in.active
                          {$specification}
                        #tab-CustomField.tab-pane
                          {$customfield}
                        #tab-Filter.tab-pane
                          {$filter}
              #tab-QtyPrice.tab-pane
                .col-lg-12.col-md-12.col-sm-12
                  .main-box.clearfix
                    .tabs-wrapper.profile-tabs
                      ul.nav.nav-tabs
                        li.active
                          a(data-toggle='tab', href='#tab-basic-pice') Basic Price
                        li
                          a(data-toggle='tab', href='#tab-qty-set-contition') Rate Chart
                      .tab-content
                        #tab-basic-pice.tab-pane.fade.in.active
                          {qty_price_detail}
                          .row
                            .col-lg-12
                              header.main-box-header.clearfix
                                h2 Price & Quantity
                              .main-box-body.clearfix
                                .row
                                  .col-lg-3
                                    label  
                                    strong Sales Price: 
                                  .col-lg-3  {sale_price} $600 {/}
                                  .col-lg-3
                                    label  
                                    strong  Original Price: 
                                  .col-lg-3 {original_price} $800 {/}
                                .xepan-push
                                .row
                                  .col-lg-3
                                    label
                                    strong Min Order Quantity:
                                  .col-lg-3  {minimum_order_qty} $600 {/}
                                  .col-lg-3
                                    label  
                                    strong  Max Order Quantity: 
                                  .col-lg-3  {maximum_order_qty} $700 {/}
                                .xepan-push
                                .row
                                  .col-lg-3
                                    label  
                                    strong Quantity Unit:
                                  .col-lg-3  {qty_unit} 70 {/}
                                  .col-lg-3
                                    label  
                                    strong Qty From Set Only:
                                  .col-lg-3
                                    .checkbox-nice {$qty_from_set_only}
                        {/}
                        #tab-qty-set-contition.tab-pane
                          {$qtysetcondition}
              #tab-Category.tab-pane
                {category}{/}
                {$cat_asso_form}
              #tab-Extra.tab-pane
                .col-lg-12.col-md-12.col-sm-12
                  .main-box.clearfix
                    .tabs-wrapper.profile-tabs
                      ul.nav.nav-tabs
                        li.active
                          a(data-toggle='tab', href='#tab-Media') Media
                        li
                          a(data-toggle='tab', href='#tab-SEO') SEO
                      .tab-content
                        #tab-Media.tab-pane.fade.in.active
                          {$media}
                        #tab-SEO.tab-pane
                          | {seo}
                          .row
                            .col-lg-12
                              .main-box.clearfix
                                .xepan-push
                                .row
                                  .col-lg-12
                                    .col-lg-4
                                      label  
                                      strong Meta Title:
                                    .col-lg-8  {meta_title} $600 {/}
                                .xepan-push
                                .row
                                  .col-lg-12
                                    .col-lg-4
                                      label  
                                      strong Meta Decription:
                                    .col-lg-8  {meta_description} $7000000000 {/}
                                .xepan-push
                                .row
                                  .col-lg-12
                                    .col-lg-4
                                      label  
                                      strong Tags:
                                    .col-lg-8  {tags} 70000000000 {/}
                                .xepan-push
                              .row
                                .col-lg-12
                                  a.btn.btn-primary.pull-right(href='#')  Update
                          | {/}
              #tab-ProductionPhase.tab-pane
                {$department}  
                {$item_dept_asso_form}
              #tab-Accounts.tab-pane
                .row
                  .col-lg-12
                    header.main-box-header.clearfix
                      .filter-block.pull-right
                        a.btn.btn-primary.pull-right(href='#')
                          i.fa.fa-plus-circle.fa-lg
                          |  Add Item Tax Association
                    .main-box-body.clearfix
                      .table-responsive
                        table.table
                          thead
                            tr
                              th.text-left(rowspan='1', colspan='1', style='width: 150px;')
                                a.asc(href='#')
                                  span Tax
                              th.text-left(style='width: 150px;')
                                a.asc(href='#')
                                  span Tax Association
                              th
                          tbody
                            tr(style='width: 150px;')
                              td
                                span Vat 0.5%
                              td.text-left 50
                              td(style='width: 15%;')
                                a.table-link(href='#')
                                  span.fa-stack
                                    i.fa.fa-square.fa-stack-2x
                                    i.fa.fa-pencil.fa-stack-1x.fa-inverse
                                a.table-link.danger(href='#')
                                  span.fa-stack
                                    i.fa.fa-square.fa-stack-2x
                                    i.fa.fa-trash-o.fa-stack-1x.fa-inverse